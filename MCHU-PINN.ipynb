    "import pandas as pd\n",
    "import numpy as np\n",
    "import torch\n",
    "import random\n",
    "torch.manual_seed(42)\n",
    "np.random.seed(42)\n",
    "random.seed(42)\n",
    "def to_tensor(x):\n",
    "    return torch.tensor(x, dtype=torch.float32)\n",
    "\n",
    "# ==== ÂèØËá™ÂÆö‰πâÂêÑÈÉ®ÂàÜÈááÊ†∑Êï∞ÈáèÔºàÂàÜÂå∫ËÆæÁΩÆÔºåÂçï‰ΩçÔºöÁÇπÊï∞Ôºâ====\n",
    "N_IC      = 6000                  # ÂàùÂßãÊù°‰ª∂ÁÇπ\n",
    "N_DATA    = 2000                 # Êï∞ÊçÆÁõëÁù£ÁÇπ\n",
    "\n",
    "# Dirichlet/Neumann/PDEÂàÜÊÆµÈááÊ†∑Ôºà6sÂÜÖ/Â§ñÔºâ\n",
    "N_BC_IN   = 1000     # 6sÂÜÖ Dirichlet\n",
    "N_BC_OUT  = 4000      # 6sÂ§ñ Dirichlet\n",
    "N_NEU_IN  = 1000     # 6sÂÜÖ Neumann\n",
    "N_NEU_OUT = 6000      # 6sÂ§ñ Neumann\n",
    "N_FAULT_IN  = 7000   # 6sÂÜÖ PDEÔºàÊñ≠Â±ÇÂå∫Ôºâ\n",
    "N_FAULT_OUT = 10000   # 6sÂ§ñ PDEÔºàÊñ≠Â±ÇÂå∫Ôºâ\n",
    "N_REST_IN   = 3000   # 6sÂÜÖ PDEÔºàÈùûÊñ≠Â±ÇÂå∫Ôºâ\n",
    "N_REST_OUT  = 20000   # 6sÂ§ñ PDEÔºàÈùûÊñ≠Â±ÇÂå∫Ôºâ\n",
    "\n",
    "# ‰∏çÁ°ÆÂÆöÊÄßÊ≠£ÂàôÁÇπÔºàÂàÜ6sÂÜÖ/Â§ñÔºâ\n",
    "N_UQ_IN = 0\n",
    "N_UQ_OUT = 8000\n",
    "\n",
    "# ==== Êï∞ÊçÆËØªÂèñ ====\n",
    "df_full = pd.read_csv(\"../autodl-tmp/dataset_pinn.csv\")\n",
    "df = df_full[df_full[\"t\"] <= 60].copy()\n",
    "\n",
    "# ==== ËæπÁïåÂÆö‰πâ ====\n",
    "X_MIN, X_MAX = 0.0, 10.0\n",
    "Y_MIN, Y_MAX = 0.0, 6.0\n",
    "\n",
    "# ==== 1. ÂàùÂßãÊù°‰ª∂ÁÇπÔºàt = 0ÔºâÔºåÂâîÈô§ Dirichlet Âíå Neumann ËæπÁïåÁÇπ ====\n",
    "df_ic = df[df['t'] == 0.0].copy()\n",
    "is_dirichlet_ic = (np.abs(df_ic['y'] - Y_MAX) < 1e-5) | \\\n",
    "                  ((df_ic['y'] < 1e-5) & df_ic['x'].between(4.0, 6.0))\n",
    "is_neumann_ic = (df_ic['x'] < 1e-5) | \\\n",
    "                (np.abs(df_ic['x'] - X_MAX) < 1e-5) | \\\n",
    "                ((df_ic['y'] < 1e-5) & ((df_ic['x'] < 4.0) | (df_ic['x'] > 6.0)))\n",
    "df_ic = df_ic[~(is_dirichlet_ic | is_neumann_ic)]\n",
    "if N_IC < len(df_ic):\n",
    "    df_ic = df_ic.sample(n=N_IC, random_state=1)\n",
    "X_ic = to_tensor(df_ic[['x', 'y', 't']].values)\n",
    "h_ic = to_tensor(np.ones((X_ic.shape[0], 1), dtype=np.float32))\n",
    "\n",
    "# ==== 2. Dirichlet ËæπÁïåÁÇπÔºà6sÂÜÖ/Â§ñÂàÜÂå∫Ôºâ====\n",
    "bc_top = np.abs(df['y'] - Y_MAX) < 1e-5\n",
    "bc_bottom_fault = (df['y'] < 1e-5) & df['x'].between(4.0, 6.0)\n",
    "df_bc = df[(bc_top | bc_bottom_fault)].copy()\n",
    "df_bc_in = df_bc[df_bc[\"t\"] <= 6.0]\n",
    "df_bc_out = df_bc[df_bc[\"t\"] > 6.0]\n",
    "df_bc_in = df_bc_in.sample(n=min(N_BC_IN, len(df_bc_in)), random_state=12)\n",
    "df_bc_out = df_bc_out.sample(n=min(N_BC_OUT, len(df_bc_out)), random_state=13)\n",
    "df_bc_final = pd.concat([df_bc_in, df_bc_out])\n",
    "X_bc = to_tensor(df_bc_final[['x', 'y', 't']].values)\n",
    "h_bc = to_tensor(df_bc_final['h'].values.reshape(-1, 1))\n",
    "\n",
    "# ==== 3. Neumann ËæπÁïåÁÇπÔºà6sÂÜÖ/Â§ñÂàÜÂå∫ÔºâÔºåÂâîÈô§ Dirichlet ÈáçÂêàÁÇπ ====\n",
    "neu_left = df['x'] < 1e-5\n",
    "neu_right = np.abs(df['x'] - X_MAX) < 1e-5\n",
    "neu_bottom = (df['y'] < 1e-5) & ((df['x'] < 4.0) | (df['x'] > 6.0))\n",
    "df_neu_all = df[(neu_left | neu_right | neu_bottom)].copy()\n",
    "bc_set = set(df_bc_final[['x', 'y', 't']].apply(tuple, axis=1))\n",
    "df_neu = df_neu_all[~df_neu_all[['x', 'y', 't']].apply(tuple, axis=1).isin(bc_set)]\n",
    "df_neu_in = df_neu[df_neu[\"t\"] <= 6.0]\n",
    "df_neu_out = df_neu[df_neu[\"t\"] > 6.0]\n",
    "df_neu_in = df_neu_in.sample(n=min(N_NEU_IN, len(df_neu_in)), random_state=14)\n",
    "df_neu_out = df_neu_out.sample(n=min(N_NEU_OUT, len(df_neu_out)), random_state=15)\n",
    "df_neu_final = pd.concat([df_neu_in, df_neu_out])\n",
    "X_neu = to_tensor(df_neu_final[['x', 'y', 't']].values)\n",
    "\n",
    "# ==== 4. PDE ÊéßÂà∂ÁÇπÔºà6sÂÜÖ/Â§ñÔºåÂâîÈô§IC/BC/NeumannÔºâ====\n",
    "bc_coords = df_bc_final[['x', 'y', 't']].drop_duplicates()\n",
    "neu_coords = df_neu_final[['x', 'y', 't']].drop_duplicates()\n",
    "ic_coords = df_ic[['x', 'y', 't']].drop_duplicates()\n",
    "df_pde_candidates = df.copy()\n",
    "df_pde_candidates = df_pde_candidates.merge(bc_coords, on=[\"x\", \"y\", \"t\"], how='left', indicator=True)\n",
    "df_pde_candidates = df_pde_candidates[df_pde_candidates[\"_merge\"] == \"left_only\"].drop(columns=[\"_merge\"])\n",
    "df_pde_candidates = df_pde_candidates.merge(neu_coords, on=[\"x\", \"y\", \"t\"], how='left', indicator=True)\n",
    "df_pde_candidates = df_pde_candidates[df_pde_candidates[\"_merge\"] == \"left_only\"].drop(columns=[\"_merge\"])\n",
    "df_pde_candidates = df_pde_candidates.merge(ic_coords, on=[\"x\", \"y\", \"t\"], how='left', indicator=True)\n",
    "df_pde_candidates = df_pde_candidates[df_pde_candidates[\"_merge\"] == \"left_only\"].drop(columns=[\"_merge\"])\n",
    "# ÂàÜÂå∫\n",
    "df_pde_in = df_pde_candidates[df_pde_candidates[\"t\"] <= 6.0]\n",
    "df_pde_out = df_pde_candidates[df_pde_candidates[\"t\"] > 6.0]\n",
    "\n",
    "df_fault_zone_in = df_pde_in[(df_pde_in[\"x\"].between(4.0, 6.0)) & (df_pde_in[\"y\"] <= 2.0)]\n",
    "df_rest_zone_in = df_pde_in[~df_pde_in.index.isin(df_fault_zone_in.index)]\n",
    "df_fault_zone_out = df_pde_out[(df_pde_out[\"x\"].between(4.0, 6.0)) & (df_pde_out[\"y\"] <= 2.0)]\n",
    "df_rest_zone_out = df_pde_out[~df_pde_out.index.isin(df_fault_zone_out.index)]\n",
    "\n",
    "df_fault_zone_in = df_fault_zone_in.sample(n=min(N_FAULT_IN, len(df_fault_zone_in)), random_state=21)\n",
    "df_rest_zone_in = df_rest_zone_in.sample(n=min(N_REST_IN, len(df_rest_zone_in)), random_state=22)\n",
    "df_fault_zone_out = df_fault_zone_out.sample(n=min(N_FAULT_OUT, len(df_fault_zone_out)), random_state=23)\n",
    "df_rest_zone_out = df_rest_zone_out.sample(n=min(N_REST_OUT, len(df_rest_zone_out)), random_state=24)\n",
    "\n",
    "df_f = pd.concat([df_fault_zone_in, df_rest_zone_in, df_fault_zone_out, df_rest_zone_out])\n",
    "X_f = to_tensor(df_f[[\"x\", \"y\", \"t\"]].values)\n",
    "\n",
    "# ==== 5. Êï∞ÊçÆÁõëÁù£ÁÇπÔºàt ‚àà {0, 2, 4, 6}ÔºâÔºåNeumann Âå∫Âüü 1000 + ÂÖ∂‰ªñÂå∫Âüü 1000 ==== \n",
    "df_supervised = df[df[\"t\"].isin([0.0, 2.0, 4.0, 6.0])].copy()\n",
    "\n",
    "neu_left_data = df_supervised[\"x\"] < 1e-5\n",
    "neu_right_data = np.abs(df_supervised[\"x\"] - X_MAX) < 1e-5\n",
    "neu_bottom_data = (df_supervised[\"y\"] < 1e-5) & (\n",
    "    (df_supervised[\"x\"] < 4.0) | (df_supervised[\"x\"] > 6.0)\n",
    ")\n",
    "neu_mask_data = neu_left_data | neu_right_data | neu_bottom_data\n",
    "\n",
    "df_data_neu_candidates  = df_supervised[neu_mask_data].copy()\n",
    "df_data_rest_candidates = df_supervised[~neu_mask_data].copy()\n",
    "\n",
    "N_DATA_NEU_TARGET = N_DATA // 2          # 1000\n",
    "n_neu_avail = len(df_data_neu_candidates)\n",
    "n_neu_pick = min(N_DATA_NEU_TARGET, n_neu_avail)\n",
    "\n",
    "df_data_neu = df_data_neu_candidates.sample(n=n_neu_pick, random_state=2024)\n",
    "\n",
    "N_DATA_REST_TARGET = N_DATA - n_neu_pick\n",
    "n_rest_avail = len(df_data_rest_candidates)\n",
    "n_rest_pick = min(N_DATA_REST_TARGET, n_rest_avail)\n",
    "\n",
    "df_data_rest = df_data_rest_candidates.sample(n=n_rest_pick, random_state=2025)\n",
    "\n",
    "df_data = pd.concat([df_data_neu, df_data_rest], axis=0)\n",
    "df_data = df_data.sample(frac=1.0, random_state=2026).reset_index(drop=True)\n",
    "\n",
    "X_data = to_tensor(df_data[[\"x\", \"y\", \"t\"]].values)\n",
    "h_data = to_tensor(df_data[\"h\"].values.reshape(-1, 1))\n",
    "\n",
    "# ==== 6. ‰∏çÁ°ÆÂÆöÊÄßÊ≠£ÂàôÁÇπÔºàUQÁÇπÔºå6sÂÜÖ/Â§ñÔºåÂêÑËá™ÂâîÈô§ÊâÄÊúâ‰∏äÈù¢Áî®ËøáÁöÑÁÇπÔºâ====\n",
    "used_coords = pd.concat([\n",
    "    df_ic[['x', 'y', 't']],\n",
    "    df_bc_final[['x', 'y', 't']],\n",
    "    df_neu_final[['x', 'y', 't']],\n",
    "    df_f[['x', 'y', 't']],\n",
    "    df_data[['x', 'y', 't']]\n",
    "]).drop_duplicates()\n",
    "\n",
    "df_uq_candidates = df.copy()\n",
    "df_uq_candidates = df_uq_candidates.merge(used_coords, on=[\"x\", \"y\", \"t\"], how='left', indicator=True)\n",
    "df_uq_candidates = df_uq_candidates[df_uq_candidates[\"_merge\"] == \"left_only\"].drop(columns=[\"_merge\"])\n",
    "\n",
    "# 6sÂÜÖ\n",
    "df_uq_in_candidates = df_uq_candidates[df_uq_candidates[\"t\"] <= 6.0]\n",
    "df_uq_in = df_uq_in_candidates.sample(n=min(N_UQ_IN, len(df_uq_in_candidates)), random_state=555)\n",
    "# 6sÂ§ñ\n",
    "df_uq_out_candidates = df_uq_candidates[df_uq_candidates[\"t\"] > 6.0]\n",
    "df_uq_out = df_uq_out_candidates.sample(n=min(N_UQ_OUT, len(df_uq_out_candidates)), random_state=556)\n",
    "# ÂêàÂπ∂\n",
    "df_uq_final = pd.concat([df_uq_in, df_uq_out])\n",
    "X_uq = to_tensor(df_uq_final[[\"x\", \"y\", \"t\"]].values)\n",
    "\n",
    "# ==== ÊâìÂç∞ÁªüËÆ°‰ø°ÊÅØ ====\n",
    "print(\"‚úÖ Êï∞ÊçÆÂàíÂàÜÂÆåÊàêÔºàt ‚àà [0, 60]ÔºåÂàÜÊÆµÈááÊ†∑ÔºâÔºö\")\n",
    "print(f\"ÂàùÂßãÊù°‰ª∂ÁÇπÊï∞ÔºàICÔºâ:            {X_ic.shape[0]}ÔºàÂÜÖÈÉ®ÁÇπÔºåh=1mÔºâ\")\n",
    "print(f\"DirichletËæπÁïåÁÇπÔºàBCÔºâ:        {X_bc.shape[0]}Ôºà6sÂÜÖ: {df_bc_in.shape[0]}Ôºå6sÂ§ñ: {df_bc_out.shape[0]}Ôºâ\")\n",
    "print(f\"NeumannÊó†ÊµÅËæπÁïåÁÇπ:           {X_neu.shape[0]}Ôºà6sÂÜÖ: {df_neu_in.shape[0]}Ôºå6sÂ§ñ: {df_neu_out.shape[0]}Ôºâ\")\n",
    "print(f\"PDEÊÆãÂ∑ÆÁÇπÔºàCollocationÔºâ:     {X_f.shape[0]}Ôºà6sÂÜÖ-fault: {df_fault_zone_in.shape[0]}, 6sÂÜÖ-rest: {df_rest_zone_in.shape[0]}, 6sÂ§ñ-fault: {df_fault_zone_out.shape[0]}, 6sÂ§ñ-rest: {df_rest_zone_out.shape[0]})\")\n",
    "print(f\"ÁõëÁù£Êï∞ÊçÆÁÇπÔºàData PointsÔºâ:    {X_data.shape[0]}Ôºàt=0,2,4,6Ôºâ\")\n",
    "print(f\"‰∏çÁ°ÆÂÆöÊÄßÊ≠£ÂàôÁÇπÔºàUQÔºâ:         {X_uq.shape[0]}Ôºà6sÂÜÖ: {df_uq_in.shape[0]}Ôºå6sÂ§ñ: {df_uq_out.shape[0]}Ôºâ\")"
