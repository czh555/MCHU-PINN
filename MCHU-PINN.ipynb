    "import numpy as np\n",
    "import pandas as pd\n",
    "import torch\n",
    "import random\n",
    "from sklearn.metrics import mean_squared_error\n",
    "\n",
    "# ========= 0. Âõ∫ÂÆöÈöèÊú∫ÁßçÂ≠êÔºàÂèØÈÄâÔºâ =========\n",
    "torch.manual_seed(42)\n",
    "np.random.seed(42)\n",
    "random.seed(42)\n",
    "model_dropout = PINN_MC([3, 128, 128, 128, 1], dropout_p=0.3)\n",
    "model_dropout.load_state_dict(torch.load(\"pinn_mchu.pt(8000,20)\", map_location=\"cpu\"))\n",
    "# ========= 1. Âä†ËΩΩÊï∞ÊçÆÔºö‰ªÖÂèñ t > 6s ‰Ωú‰∏∫‚ÄúÈ¢ÑÊµãÂüü‚Äù =========\n",
    "df = pd.read_csv(\"../autodl-tmp/dataset_pinn.csv\")  # Ë∑ØÂæÑÊåâ‰Ω†‰πãÂâçÁöÑÊù•ÔºåÂøÖË¶ÅÊó∂Êîπ‰∏Ä‰∏ã\n",
    "\n",
    "test_df = df[df[\"t\"] > 6.0].copy()\n",
    "print(\"ÊµãËØïÂüüÊÄªÁÇπÊï∞Ôºàt>6sÔºâÔºö\", len(test_df))\n",
    "\n",
    "X_test = torch.tensor(test_df[[\"x\", \"y\", \"t\"]].values, dtype=torch.float32)\n",
    "h_test = torch.tensor(test_df[[\"h\"]].values, dtype=torch.float32)\n",
    "\n",
    "# ========= 2. ‰ªé t>6s ‰∏≠ÂàíÂàÜ 5000 Ê†°ÂáÜÈõÜÔºåÂÖ∂‰Ωô‰∏∫ËØÑ‰º∞ÈõÜ =========\n",
    "N_total = X_test.shape[0]\n",
    "N_calib = 1000\n",
    "\n",
    "idx = np.arange(N_total)\n",
    "np.random.seed(2025)\n",
    "np.random.shuffle(idx)\n",
    "\n",
    "idx_calib = idx[:N_calib]\n",
    "idx_eval  = idx[N_calib:]\n",
    "\n",
    "X_calib = X_test[idx_calib]\n",
    "y_calib = h_test[idx_calib]\n",
    "\n",
    "X_eval  = X_test[idx_eval]\n",
    "y_eval  = h_test[idx_eval]\n",
    "\n",
    "print(f\"Ê†°ÂáÜÈõÜÂ§ßÂ∞è: {X_calib.shape[0]}  |  ËØÑ‰º∞ÈõÜÂ§ßÂ∞è: {X_eval.shape[0]}\")\n",
    "\n",
    "# ========= 3. ÂÆö‰πâ MC Dropout Êé®Êñ≠ÂáΩÊï∞ =========\n",
    "def mc_dropout_predict(model, X, mc_times=100, device=None):\n",
    "    \"\"\"\n",
    "    ÂØπÁªôÂÆöÊ®°ÂûãÂíåËæìÂÖ• X ÂÅö mc_times Ê¨° Dropout ÂâçÂêëÔºåËøîÂõû:\n",
    "    mean: [N, 1]\n",
    "    std:  [N, 1]\n",
    "    \"\"\"\n",
    "    if device is None:\n",
    "        device = next(model.parameters()).device\n",
    "\n",
    "    model.train()  # ÂÖ≥ÈîÆÔºöËÆ© Dropout ÁîüÊïà\n",
    "    X = X.to(device)\n",
    "\n",
    "    preds = []\n",
    "    with torch.no_grad():\n",
    "        for _ in range(mc_times):\n",
    "            pred = model(X)                  # [N, 1]\n",
    "            preds.append(pred.unsqueeze(0))  # [1, N, 1]\n",
    "\n",
    "    preds = torch.cat(preds, dim=0)      # [mc_times, N, 1]\n",
    "    mean  = preds.mean(dim=0)            # [N, 1]\n",
    "    std   = preds.std(dim=0)             # [N, 1]\n",
    "    return mean, std\n",
    "\n",
    "# ========= 4. PICP / MPIW ËÆ°ÁÆó + Ê∏©Â∫¶ÊêúÁ¥¢ =========\n",
    "def picp_mpiw(y_true, mu, sigma, T=1.0, alpha=0.95):\n",
    "    \"\"\"\n",
    "    ËÆ°ÁÆó PICPÔºàË¶ÜÁõñÁéáÔºâÂíå MPIWÔºàÂπ≥ÂùáÂå∫Èó¥ÂÆΩÂ∫¶Ôºâ\n",
    "    y_true, mu, sigma: 1D numpy Êï∞ÁªÑÊàñÂèØËΩ¨ np ÁöÑÂêëÈáè\n",
    "    T: Ê∏©Â∫¶Áº©ÊîæÁ≥ªÊï∞\n",
    "    alpha: ÁΩÆ‰ø°Ê∞¥Âπ≥Ôºå‰æãÂ¶Ç 0.95\n",
    "    \"\"\"\n",
    "    y_true = np.asarray(y_true).reshape(-1)\n",
    "    mu     = np.asarray(mu).reshape(-1)\n",
    "    sigma  = np.asarray(sigma).reshape(-1)\n",
    "\n",
    "    z = 1.96  # Ê≠£ÊÄÅÂàÜÂ∏É‰∏ã 95% ÂàÜ‰Ωç\n",
    "\n",
    "    width = z * T * sigma\n",
    "    lower = mu - width\n",
    "    upper = mu + width\n",
    "\n",
    "    inside = (y_true >= lower) & (y_true <= upper)\n",
    "    picp = inside.mean()\n",
    "    mpiw = (upper - lower).mean()\n",
    "\n",
    "    return float(picp), float(mpiw)\n",
    "\n",
    "def find_best_temperature(y_calib, mu_calib, sigma_calib, alpha=0.95):\n",
    "    \"\"\"\n",
    "    Âú®‰∏ÄÁª¥ÁΩëÊ†º‰∏äÊêúÁ¥¢ TÔºå‰Ωø PICP(T) ÊúÄÊé•Ëøë alpha\n",
    "    \"\"\"\n",
    "    Ts = np.linspace(0.5, 4.0, 71)  # [0.5, 3.0] ‰∏≠ 51 ‰∏™ÁÇπ\n",
    "    best_T = 1.0\n",
    "    best_gap = 1e9\n",
    "    best_picp = None\n",
    "\n",
    "    for T in Ts:\n",
    "        picp, _ = picp_mpiw(y_calib, mu_calib, sigma_calib, T=T, alpha=alpha)\n",
    "        gap = abs(picp - alpha)\n",
    "        if gap < best_gap:\n",
    "            best_gap = gap\n",
    "            best_T = T\n",
    "            best_picp = picp\n",
    "\n",
    "    print(f\"üîß Ê†°ÂáÜÈõÜ: ÁõÆÊ†áË¶ÜÁõñÁéá alpha={alpha}, ÊúÄ‰ºò T={best_T:.3f}, PICP={best_picp:.4f}\")\n",
    "    return best_T, best_picp\n",
    "\n",
    "# ========= 5. Âä†ËΩΩËÆ≠ÁªÉÂ•ΩÁöÑ MCHU-PINN Ê®°Âûã =========\n",
    "device = torch.device(\"cuda\" if torch.cuda.is_available() else \"cpu\")\n",
    "\n",
    "# ÂÅáËÆæÁΩëÁªúÁªìÊûÑ‰∏éËÆ≠ÁªÉÊó∂‰∏ÄËá¥\n",
    "model_mchu = PINN_MC([3, 128, 128, 128, 1], dropout_p=0.3)\n",
    "\n",
    "# Â¶ÇÊñá‰ª∂‰∏çÂú®ÂΩìÂâçÁõÆÂΩïÔºåËØ∑ÊîπÊàêÂÆûÈôÖË∑ØÂæÑÔºå‰æãÂ¶Ç \"../autodl-tmp/pinn_mchu.pt\"\n",
    "state_dict_mchu = torch.load(\"pinn_mchu.pt(8000,20)\", map_location=device)\n",
    "model_mchu.load_state_dict(state_dict_mchu)\n",
    "model_mchu.to(device)\n",
    "\n",
    "print(\"‚úÖ Â∑≤Âä†ËΩΩËÆ≠ÁªÉÂ•ΩÁöÑ MCHU-PINN (model_mchu) Âπ∂ÊîæÂà∞ËÆæÂ§á:\", device)\n",
    "\n",
    "# ========= 6. Âú®Ê†°ÂáÜÈõÜ / ËØÑ‰º∞ÈõÜ‰∏äÂÅö MC Dropout =========\n",
    "mc_times = 100  # Âíå MCH-PINN ‰øùÊåÅ‰∏ÄËá¥\n",
    "\n",
    "with torch.no_grad():\n",
    "    mu_calib_mchu, std_calib_mchu = mc_dropout_predict(\n",
    "        model_mchu, X_calib, mc_times=mc_times, device=device\n",
    "    )\n",
    "    mu_eval_mchu,  std_eval_mchu  = mc_dropout_predict(\n",
    "        model_mchu, X_eval,  mc_times=mc_times, device=device\n",
    "    )\n",
    "\n",
    "# ËΩ¨‰∏∫ numpy\n",
    "mu_calib_mchu_np  = mu_calib_mchu.cpu().numpy().reshape(-1)\n",
    "std_calib_mchu_np = std_calib_mchu.cpu().numpy().reshape(-1)\n",
    "y_calib_np        = y_calib.cpu().numpy().reshape(-1)\n",
    "\n",
    "mu_eval_mchu_np   = mu_eval_mchu.cpu().numpy().reshape(-1)\n",
    "std_eval_mchu_np  = std_eval_mchu.cpu().numpy().reshape(-1)\n",
    "y_eval_np         = y_eval.cpu().numpy().reshape(-1)\n",
    "\n",
    "# ========= 7. ÂéüÂßã (T=1) ÊÉÖÂÜµ‰∏ãÁöÑ PICP / MPIW =========\n",
    "picp_raw_calib_mchu, mpiw_raw_calib_mchu = picp_mpiw(\n",
    "    y_calib_np, mu_calib_mchu_np, std_calib_mchu_np, T=1.0, alpha=0.95\n",
    ")\n",
    "picp_raw_eval_mchu,  mpiw_raw_eval_mchu  = picp_mpiw(\n",
    "    y_eval_np,  mu_eval_mchu_np,  std_eval_mchu_np,  T=1.0, alpha=0.95\n",
    ")\n",
    "\n",
    "print(\"\\n=== MCHU-PINN ÂéüÂßã (T=1) ÊÉÖÂÜµ ===\")\n",
    "print(f\"Ê†°ÂáÜÈõÜ: PICP = {picp_raw_calib_mchu:.4f}, MPIW = {mpiw_raw_calib_mchu:.4f}\")\n",
    "print(f\"ËØÑ‰º∞ÈõÜ: PICP = {picp_raw_eval_mchu:.4f},  MPIW = {mpiw_raw_eval_mchu:.4f}\")\n",
    "\n",
    "# ========= 8. Âú®Ê†°ÂáÜÈõÜ‰∏äÊêúÁ¥¢ÊúÄ‰ºò T =========\n",
    "T_mchu, picp_calib_best_mchu = find_best_temperature(\n",
    "    y_calib_np, mu_calib_mchu_np, std_calib_mchu_np, alpha=0.95\n",
    ")\n",
    "\n",
    "# ========= 9. Áî®ÊúÄ‰ºò T Âú®Ê†°ÂáÜÈõÜ / ËØÑ‰º∞ÈõÜ‰∏äÈáçÊñ∞ËÆ°ÁÆó PICP / MPIW =========\n",
    "picp_calib_T_mchu, mpiw_calib_T_mchu = picp_mpiw(\n",
    "    y_calib_np, mu_calib_mchu_np, std_calib_mchu_np, T=T_mchu, alpha=0.95\n",
    ")\n",
    "picp_eval_T_mchu,  mpiw_eval_T_mchu  = picp_mpiw(\n",
    "    y_eval_np,  mu_eval_mchu_np,  std_eval_mchu_np,  T=T_mchu, alpha=0.95\n",
    ")\n",
    "\n",
    "print(\"\\n=== Ê∏©Â∫¶Áº©ÊîæÂêé (Calibrated) MCHU-PINN ===\")\n",
    "print(f\"ÊúÄ‰ºò T = {T_mchu:.3f}\")\n",
    "print(f\"Ê†°ÂáÜÈõÜ: PICP = {picp_calib_T_mchu:.4f}, MPIW = {mpiw_calib_T_mchu:.4f}\")\n",
    "print(f\"ËØÑ‰º∞ÈõÜ: PICP = {picp_eval_T_mchu:.4f},  MPIW = {mpiw_eval_T_mchu:.4f}\")\n",
    "\n",
    "# ========= 10. ËØÑ‰º∞ÈõÜ RMSEÔºàMC ÂùáÂÄº‰Ωú‰∏∫ÁÇπÈ¢ÑÊµãÔºâ =========\n",
    "rmse_eval_mchu = np.sqrt(mean_squared_error(y_eval_np, mu_eval_mchu_np))\n",
    "print(f\"\\nüìè MCHU-PINN ËØÑ‰º∞ÈõÜ RMSE (‰ΩøÁî® MC Âπ≥Âùá‰Ωú‰∏∫ÁÇπÈ¢ÑÊµã): {rmse_eval_mchu:.4f} m\")\n"
   ]
